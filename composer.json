{
  "name": "samsoncms/newsletter",
  "description": "Newsletter management for SamsonCMS",
  "keywords": [
    "resource",
    "file"
  ],
  "authors": [
    {
      "name": "Pavlo Onysko",
      "email": "onysko@samsonos.com"
    }
  ],
  "require": {
    "samsoncms/cms": "*",
    "samsoncms/application": "*",
    "samsonos/php_activerecord": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "minimum-stability": "dev",
  "license": "Open Software License (OSL) v 3.0",
  "homepage": "http://samsonos.com/",
  "autoload": {
    "classmap": ["src/"],
    "psr-4": {
      "samsonframework\\resource\\": "src/"
    }
  },
  "config":{
    "github-protocols":["https"]
  },
  "extra": { 
    "samsonphp_package_compressable": "1", 
    "samson_module_include": "1"
  }
}
